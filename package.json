    "eleventy-plugin-mathjax": "^2.0.4",
    "eleventy-plugin-reader-bar": "^0.2.1",
    "eleventy-plugin-toc": "^1.1.5",
    "eleventy-plugin-typeset": "^1.0.0",
    "luxon": "^3.5.0",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "sass": "^1.82.0"
  },
